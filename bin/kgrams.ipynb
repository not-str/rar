    "def generate_kgrams(term,k): \n",
    "    kgrams=set() \n",
    "    padded_term=f\"${term}$\" \n",
    "    for i in range(len(padded_term)-k+1): \n",
    "        kgrams.add(padded_term[i:i+k]) \n",
    "    return kgrams \n",
    "term=\"siescollege\" \n",
    "k=3 \n",
    "print(generate_kgrams(term,k))"
   ]
  }
