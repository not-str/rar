    "import networkx as nx \n",
    "G=nx.DiGraph()    \n",
    "G.add_edges_from([(1,2),(1,3),(2,4),(3,4),(4,5)])  \n",
    "authority_scores,hub_scores=nx.hits(G) \n",
    "print(\"Authority Scores=\",authority_scores) \n",
    "print(\"Hub scores\",hub_scores)"
   ]
